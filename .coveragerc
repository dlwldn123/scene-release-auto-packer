[run]
source = web,src,tests/phase0
omit = 
    */tests/*
    */venv/*
    */__pycache__/*
    */migrations/*
    web/app.py
    tests/e2e/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov


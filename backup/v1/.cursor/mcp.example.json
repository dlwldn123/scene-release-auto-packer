{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@context7/cli", "mcp", "--stdio"],
      "env": {}
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@browserbasehq/mcp-playwright", "start", "--stdio"],
      "env": {
        "HEADLESS": "1"
      }
    }
  }
}


